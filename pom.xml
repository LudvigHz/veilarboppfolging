                <groupId>no.nav.sbl.dialogarena</groupId>
                <artifactId>ytelseskontrakt-v3-tjenestespesifikasjon</artifactId>
                <version>${ytelseskontrakt-v3-tjenestespesifikasjon.version}</version>
            </dependency>
            <dependency>
                <groupId>no.nav.sbl.dialogarena</groupId>
                <artifactId>oppfoelging-v1-tjenestespesifikasjon</artifactId>
                <version>${oppfoelging-v1-tjenestespesifikasjon.version}</version>
            </dependency>
            <dependency>
                <groupId>no.nav.sbl.dialogarena</groupId>
                <artifactId>dial-nav-tjeneste-aktoer_v2</artifactId>
