            <dependency>
                <groupId>no.nav.fo</groupId>
                <artifactId>veilarbaktivitet-api</artifactId>
                <version>1.181.55</version>
            </dependency>

            <dependency>
                <groupId>no.nav.fo</groupId>
                <artifactId>veilarbsituasjon-common</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>no.nav.fo</groupId>
                <artifactId>veilarbsituasjon-common</artifactId>
                <version>${project.version}</version>
