        <artifactId>mor-pom</artifactId>
        <version>3.2</version>
    </parent>

    <name>VeilArbSituasjon</name>


    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>

        <!--Tjenester-->
        <ytelseskontrakt-v3-tjenestespesifikasjon.version>1.0.0</ytelseskontrakt-v3-tjenestespesifikasjon.version>
        <oppfoelging-v1-tjenestespesifikasjon.version>1.1.0</oppfoelging-v1-tjenestespesifikasjon.version>
        <dial-nav-tjeneste-aktoer_v2.version>1.1</dial-nav-tjeneste-aktoer_v2.version>
        <dkif-tjenestespesifikasjon.version>1.2</dkif-tjenestespesifikasjon.version>
        <brukerdialog-behandleSituasjon-v1-tjenestespesifikasjon.version>1.0.7-SNAPSHOT</brukerdialog-behandleSituasjon-v1-tjenestespesifikasjon.version>
        <organisasjonenhet-v1.version>1.0.7</organisasjonenhet-v1.version>

        <javaslang.version>2.1.0-alpha</javaslang.version>

        <sonar.exclusions>
            **/mock/*,
            **/config/**
        </sonar.exclusions>
