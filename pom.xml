    <dependencyManagement>
        <dependencies>

            <dependency>
                <groupId>no.nav.sbl.dialogarena</groupId>
                <artifactId>api-app-bom</artifactId>
                <version>3.78.48</version>
                <type>pom</type>
                <scope>import</scope>
